<AnchorPane prefHeight="600.0" prefWidth="824.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.Controller.RaportController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="824.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/byRaport.png" />
         </image>
      </ImageView>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#totalProfit" prefHeight="44.0" prefWidth="153.0" style="-fx-background-color: #40cfff;" text="Общая прибыль" />
      <Button layoutX="493.0" layoutY="14.0" mnemonicParsing="false" onAction="#topService" prefHeight="44.0" prefWidth="153.0" style="-fx-background-color: #40cfff;" text="Прибыльные услуги" />
